lookup reorder_kashida {
	sub kashida-ar		by dummy.mark;
	sub @dots			by kashida-ar.dots @dots;
	sub @tashkil		by kashida-ar.dots @tashkil;
} reorder_kashida;

lookup rclt_kashida_reorder_dots {
	lookupflag UseMarkFilteringSet @dots;

	sub	@beh_init_medi
		 @dots' lookup reorder_kashida
		 kashida-ar
	;
	sub	@beh_init_medi
		 kashida-ar.dots
		 @dots
		 kashida-ar' lookup reorder_kashida
	;
} rclt_kashida_reorder_dots;

lookup rclt_kashida_reorder_fatha {
	lookupflag UseMarkFilteringSet @tashkil;

	sub	 @init_medi
		 @tashkil' lookup reorder_kashida
		 kashida-ar
	;
	sub	 @init_medi
		 kashida-ar.dots
		 @tashkil
		 kashida-ar' lookup reorder_kashida
	;
} rclt_kashida_reorder_fatha;

lookup rclt_kashida_wide_fatha {
	lookupflag UseMarkFilteringSet [fatha-ar fathatan-ar];

	sub	[tah-ar.isol tah-ar.init tah-ar.medi tah-ar.fina kaf-ar.isol kaf-ar.init kaf-ar.medi kaf-ar.fina]
		[fatha-ar fathatan-ar]'
	by	[fatha-ar.kashida.alt fathatan-ar.kashida.alt]
	;

	sub	 @mashq_glyphs
		[fatha-ar fathatan-ar]'
	by	[fatha-ar.kashida fathatan-ar.kashida]
	;
} rclt_kashida_wide_fatha;

lookup ss01 {
	sub @dots by dummy.mark;
} ss01;

